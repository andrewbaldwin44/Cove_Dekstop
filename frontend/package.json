{
  "name": "@CoveDesktop/frontend",
  "version": "1.0.0",
  "author": "Andrew Baldwin",
  "main": "dist/index.html",
  "scripts": {
    "build": "PRODUCTION=true webpack --config webpack.config.js",
    "watch": "yarn build --watch",
    "dev:web": "WEB=true webpack-dev-server --config webpack.renderer.config.js --no-client-overlay-warnings",
    "dev:renderer": "webpack-dev-server --config webpack.config.js --no-client-overlay-warnings",
    "dev:electron": "electron src/electron.ts",
    "dev": "npm-run-all --parallel dev:renderer dev:electron",
    "start:electron": "PRODUCTION=true electron ./dist/electron.js",
    "start": "npm-run-all --parallel build start:electron",
    "clean": "rm -rf ./dist",
    "type-check": "tsc",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write './src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@types/react-redux": "^7.1.23",
    "@types/redux": "^3.6.0",
    "array-move": "^3.0.1",
    "draggable": "^4.2.0",
    "electron": "^17.2.0",
    "firebase": "^7.18.0",
    "moment": "^2.29.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.4.4",
    "react-hook-form": "^7.28.0",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0",
    "react-simple-timefield": "^3.2.5",
    "react-sortable-hoc": "^2.0.0",
    "react-with-firebase-auth": "^1.3.2",
    "redux": "^4.0.5",
    "simple-peer": "^9.11.1",
    "socket.io-client": "^4.4.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "typescript": "^4.2.3",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.7.4"
  },
  "browserslist": [
    "last 5 Chrome versions",
    "last 5 Firefox versions",
    "last 3 Edge versions",
    "Safari >= 11",
    "iOS >= 11",
    ">5% in CA",
    ">5% in US"
  ]
}
